class Solution:
    def minCost(self, costs: List[List[int]]) -> int:
        n = len(costs)
        for i in range(1, n):
            for j in range(3):
                b = costs[i - 1]
                a = b.copy()
                del a[j]
                costs[i][j] += min(a)
        return min(costs[-1])
